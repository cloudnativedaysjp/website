    "devDependencies": {
        "@babel/core": "^7.20.5",
        "@material-ui/core": "^4.12.4",
        "@mui/icons-material": "^5.0.0",
        "@types/node": "18.11.17",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.10",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "autoprefixer": "^10.4.13",
        "axios": "^1.2.1",
        "babel": "^6.23.0",
